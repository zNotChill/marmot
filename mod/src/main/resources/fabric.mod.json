{
  "schemaVersion": 1,
  "id": "marmot",
  "version": "${version}",
  "name": "marmot",
  "description": "A Minecraft library to empower customization.",
  "authors": ["zNotChill", "koloja"],
  "contact": {
    "homepage": "https://discord.com/invite/ukKbph9KFy",
    "sources": "https://github.com/zNotChill/marmot"
  },
  "icon": "assets/marmot_512.png",
  "license": "All-Rights-Reserved",
  "environment": "client",
  "entrypoints": {
    "fabric-datagen": [
      "me.znotchill.marmot.client.MarmotDataGenerator"
    ],
    "client": [
      "me.znotchill.marmot.client.MarmotClient"
    ],
    "main": [
      "me.znotchill.marmot.Marmot"
    ]
  },
  "mixins": [
    "marmot.mixins.json",
    {
      "config": "marmot.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-language-kotlin": ">=${kotlin_loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },
  "suggests": {
    "modmenu": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "Discord": "https://discord.com/invite/ukKbph9KFy"
      }
    }
  }
}